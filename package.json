    "@sapphire/utilities": "^3.13.0",
    "@types/node": "^20.8.9",
    "bullmq": "^4.12.7",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "redis": "^4.6.10",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "3.1.0",
    "prisma": "^5.5.2"
